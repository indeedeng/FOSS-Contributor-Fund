{
  "name": "starfish",
  "version": "1.0.0",
  "description": "- parse a CSV of employee GitHub Ids - use those Ids and the Github REST API to check for open source contributions on GitHub that happened between 2 different dates - make a new text file with the GitHub Ids of those employees",
  "main": "contributors.js",
  "scripts": {
    "test": "jest test",
    "test:coverage": "jest test --coverage",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^8.2.0",
    "eslint-plugin-gettext": "^1.1.0",
    "luxon": "^1.25.0",
    "node-fetch": "^2.6.1",
    "parse-csv": "^0.2.0",
    "parse-link-header": "^1.0.1"
  },
  "devDependencies": {
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.5.3",
    "nock": "^13.0.4",
    "prettier": "^2.1.2",
    "sinon": "^9.2.0"
  },
  "jest": {
    "verbose": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/coverage/**"
    ],
    "coverageDirectory": "<rootDir>/coverage",
    "setupFiles": [
      "<rootDir>/.jest/setEnvVars.js"
    ]
  }
}
